								"exec": [
									"if (pm.response.code === 200 || pm.response.code === 201) {",
									"    var jsonData = pm.response.json();",
									"    if (jsonData.access) {",
									"        pm.environment.set('access_token', jsonData.access);",
									"        console.log('Access token saved:', jsonData.access);",
									"    }",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],

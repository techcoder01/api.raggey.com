						"auth": {
							"type": "noauth"
						},
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"username\": \"testuser123\",\n    \"email\": \"testuser123@example.com\",\n    \"password\": \"Test@1234\",\n    \"first_name\": \"Test\",\n    \"last_name\": \"User\",\n    \"dv_name\": \"TestDevice\",\n    \"dv_id\": \"device_123\",\n    \"dv_type\": \"mobile\",\n    \"fcm_token\": \"fcm_token_12345\"\n}"
